<div class="text-center">
  <h2>Users</h2>
  <br><br><br>
  <div class="container5">
    <% @users.each do  |user| %>
    <div class="card">
      <h3> <%= user.first_name %> </h3>
      <p><%= user.address %></p>
      <%= link_to "See", user_path(user) %>
      <%= link_to "Delete", user_path(user), method: :delete, data: {
        confirm: "Are you sure?" } %>
      </div>
      <br><br>
      <% end %>
    </div>
  </div>



